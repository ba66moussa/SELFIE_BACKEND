# Port du serveur
PORT=3000

# URL publique (pour générer le lien que vous envoyez au client)
PUBLIC_URL=http://localhost:3000

# Origines autorisées (CORS) — séparez par des virgules
ALLOWED_ORIGINS=http://localhost:3000

# Endpoint de votre plugin BLS (ex: https://blsspainmorocco.net/plugin_liveness.php)
BLS_PLUGIN_ENDPOINT=

# Credentials fournis par BLS (si requis par votre plugin)
BLS_API_KEY=
BLS_PARTNER_CODE=

# Secret pour sécuriser le webhook (optionnel)
SELFIE_WEBHOOK_SECRET=change-me
